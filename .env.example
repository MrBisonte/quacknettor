# Example environment variables for DuckEL
# Copy this file to .env and fill in your actual values
# The .env file is gitignored and will not be committed

# PostgreSQL Connection
PG_PASSWORD=your_postgres_password_here
PG_CONNECTION_STRING=dbname=mydb user=myuser host=127.0.0.1 port=5432 password=${PG_PASSWORD}

# Snowflake Connection
SF_PASSWORD=your_snowflake_password_here
SF_CONNECTION_STRING=user=myuser password=${SF_PASSWORD} account=myaccount warehouse=mywh database=mydb schema=public

# AWS S3 Credentials (optional if using IAM role)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# S3 Test Bucket (for running S3 tests)
S3_TEST_BUCKET=your-test-bucket-name

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/duckel.log
